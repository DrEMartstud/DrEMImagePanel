{
  "name": "drem-image-panel",
  "displayName": "DREM Image Panel",
  "description": "Simple image panel for VS Code",
  "version": "0.0.1",
  "publisher": "DrEMartstud",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "image",
    "panel",
    "viewer"
  ],
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/DrEMartstud/DrEMImagePanel"
  },
  "activationEvents": [
    "onCommand:drem-image-panel.showImage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "drem-image-panel.showImage",
        "title": "Show Image Panel",
        "category": "DREM Image Panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}